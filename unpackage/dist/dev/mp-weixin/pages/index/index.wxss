page{width: 100%;
	height: 100%;
	background-color: #f4f8f9;
	font-family:YouYuan, SimSun,Microsoft YaHei,monospace;
}

/*搜素框*/
#toSearch{
	position: relative;
	width: 650rpx;
	height: 80rpx;
	left: 50%;
	-webkit-transform: translateX(-50%);
	        transform: translateX(-50%);
	border-radius: 50rpx;
	border-bottom: 1rpx solid #afafaf;
	padding: 5rpx 20rpx;
}
input{
	position: absolute;
	/* text-align: left; */
	left: 20rpx;
	top: 50%;
	-webkit-transform: translateY(-50%);
	        transform: translateY(-50%);
	font-size: 20rpx;
	letter-spacing: 3rpx;
	/* float: left; */
}
#search_img{
	position: absolute;
	right: 20rpx;
	top: 50%;
	width: 40rpx;
	height: 40rpx;
	float: right;
}

/*日历*/
#time{
		width: 400rpx;
		height: 180rpx;
		background-color: #fff;
		margin: 80rpx auto;
		border-radius: 20rpx;
		box-shadow: 0 5rpx 20rpx #ddd;
}
#t_date{
		position: absolute;
		top: 22rpx;
		left: 26rpx;
		width: 250rpx;
		height: 100rpx;
		font-size: 56rpx;
		font-weight: 800;
		color: #E35087;
		/* float: left; */
		margin: 20rpx 20rpx;
}
#date_yy{
		font-size: 30rpx;
		color: #ccc;
		margin-top: 20rpx;
		letter-spacing: 10rpx;
}
#t_day{
		position: absolute;
		right: 14rpx;
		bottom: 12rpx;
		width: 180rpx;
		height: 100rpx;
		border-radius: 30rpx;
		text-align: center;
		color: #E35087;
}
#t_day ._span{
		font-size: 40rpx;
		font-weight: 800;
		letter-spacing: 10rpx;
		display: inline-block;
		height: 100rpx;
		line-height: 100rpx;
}
.gradient text{
		font-size: 30rpx;
		letter-spacing: 10rpx;
		font-weight: 800;
}
.image-bg {
		width: 100%;
		height: 100%;
}

/*选择*/
.nav {
		position: absolute;
	    top: 50%;
		left: 50%;
		-webkit-transform: translate(-50%,-27%);
		        transform: translate(-50%,-27%);
		width: 100%;
		height: 600rpx;
		/* bottom: 0; */
}
.nav_box1{
		position: absolute;
		top:50%;
		left: 50%;
		-webkit-transform: translate(-89%,-105%) rotate(343deg);
		        transform: translate(-89%,-105%) rotate(343deg);
		width: 300rpx;
		height: 250rpx;	
		border-radius: 30rpx;
		box-shadow: 0 5rpx 20rpx #2122B9;
		background-color: #2122B9;
}
.nav_box2{
		position:absolute;
	    top:50%;
	    left: 50%;
	    -webkit-transform: translate(-7%,-50%) rotate(10deg);
	            transform: translate(-7%,-50%) rotate(10deg);
		width: 300rpx;
		height: 250rpx;	
		border-radius: 30rpx;
		box-shadow: 0 5rpx 20rpx #F6C542;
		background-color: #F6C542;
		background: -webkit-linear-gradient(203deg,#F6C542 40%,  #f4f8f9 100% );
		background: linear-gradient(247deg,#F6C542 40%,  #f4f8f9 100% );
}
.nav_box3{
		z-index: 100;
		position: absolute;
		top:50%;
		left: 50%;
		-webkit-transform: translate(-50%,-50%);
		        transform: translate(-50%,-50%);
		width: 300rpx;
		height: 250rpx;
		border-radius: 50rpx;
		background: rgba(0, 0, 0, 0.1);
}
.co-center{
		position: absolute;
		top:50%;
		left: 50%;
		-webkit-transform: translate(-50%,-70%);
		        transform: translate(-50%,-70%);
		color: #fff;
		text-align: center;
		font-size: 120rpx;
}
.ch-font{
		position: absolute;
		bottom: 38rpx;
		left: 50%;
		-webkit-transform: translateX(-50%);
		        transform: translateX(-50%);
		color: #fff;
		font-size: 30rpx;
}
.mask{
		z-index: 99;
		position: absolute;
		top:50%;
		left: 50%;
		-webkit-transform: translate(-80%,17%);
		        transform: translate(-80%,17%);
		width: 300rpx;
		height: 250rpx;
		border: 20rpx solid #E35087;
		-webkit-filter:opacity(90%);
		        filter:opacity(90%);
		border-radius: 50rpx;
		box-shadow: 0 5rpx 20rpx #E35087;
}

/* 音乐组件 */
@-webkit-keyframes musicRotate {
from {
			-webkit-transformb: rotate(0deg)
}
to {
			-webkit-transform: rotate(360deg)
}
}
@-webkit-keyframes musicStop {
from {
			-webkit-transform: rotate(20deg)
}
to {
			-webkit-transform: rotate(0deg)
}
}
@-webkit-keyframes musicStart {
from {
			-webkit-transform: rotate(0deg)
}
to {
			-webkit-transform: rotate(20deg)
}
}
.small_blue{
		position: absolute;
		-webkit-transform: translate(399%,360%);
		        transform: translate(399%,360%);
		right: 50%;
		top: 50%;
		width: 50rpx;
		height: 50rpx;
		border-radius: 50%;
		background-color: #484EB5;
		-webkit-filter: blur(2px);
		        filter: blur(2px);
}
.yellow_circular{
		position: absolute;
		-webkit-transform: translate(500%,-514%);
		        transform: translate(500%,-514%);
		right: 50%;
		top: 50%;
		width: 50rpx;
		height: 50rpx;
		border-radius: 50%;
		background-color: #F6C542;
		-webkit-filter: blur(2px);
		        filter: blur(2px);
}
.blue_circular{position: absolute;
		-webkit-transform: translate(228%,-239%);
		        transform: translate(228%,-239%);
		right: 50%;
		top: 50%;
		width: 100rpx;
		height: 100rpx;
		border-radius: 50%;
		background-color:#484EB5 ;
		-webkit-filter: blur(1px);
		        filter: blur(1px);
}
.bg_music {
		position: absolute;
		-webkit-transform: translate(228%,-239%);
		        transform: translate(228%,-239%);
		right: 50%;
		top: 50%;
		width: 100rpx;
		height: 100rpx;
		border-radius: 50%;
		z-index: 99;
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		-webkit-box-pack: center;
		-webkit-justify-content: center;
		        justify-content: center;
		-webkit-box-align: center;
		-webkit-align-items: center;
		        align-items: center;
}
.musicImg {
		width: 60rpx;
		height: 60rpx;
}
.music_icon {
		-webkit-animation: musicRotate 3s linear infinite;
		        animation: musicRotate 3s linear infinite;
}
.music_play {
		width: 28rpx;
		height: 60rpx;
		margin-left: -10rpx;
		-webkit-transform-origin: top;
		        transform-origin: top;
		-webkit-transform: rotate(20deg);
}
.playImg {
		-webkit-animation: musicStop 1s linear forwards;
		        animation: musicStop 1s linear forwards;
}
.pauseImg {
		-webkit-animation: musicStart 1s linear forwards;
		        animation: musicStart 1s linear forwards;
}
#myAudio {
		display: none;
}

