/*画笔样式*/
#myPen {
	width: 452rpx;
	height: 120rpx;
	background-color: #FFFFFF;
	opacity: 0.7;
	box-shadow: 0rpx 0rpx 30rpx 1rpx #e2e2e2;
	border-radius: 30rpx;
	position: fixed;
	bottom: 225rpx;
	right: 110rpx;
}
.myColor {
	width: 45rpx;
	height: 43rpx;
	float: left;
	margin: 10rpx 16rpx;
}
.myThick {
	height: 48rpx;
	float: left;
	margin: 5rpx 34rpx;
	background-color: #333333;
}
