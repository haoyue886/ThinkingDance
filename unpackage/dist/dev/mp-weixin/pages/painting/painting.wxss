page {
	width: 100%;
	height: 100%;
	font-family: YouYuan, SimSun, Microsoft YaHei, monospace;
}
.content {
	width: 100%;
	height: 100%;
}



/*今日日期*/
.date_ {
	width: 50rpx;
	height: 50rpx;
	float: left;
	margin-right: 15rpx;
	margin-left: 15rpx;
	text-align: center;
	color: #9f9f9f;
}
#date {
	position: fixed;
	top: 40rpx;
	left: 5%;
	z-index: 10;
	border: 1px solid #5B4CB1;
	width: 178rpx;
	height: 120rpx;
}
#date_d {
	font-size: 50rpx;
	font-weight: 400;
	color: #F6C542;
}
#line {
	-webkit-transform: rotate(55deg);
	        transform: rotate(55deg);
	width: 4rpx;
	height: 120rpx;
	background-color: #5B4CB1;
}
#date_y_m {
	position: absolute;
	bottom: 0;
	right: 16rpx;
	font-size: 50rpx;
	color: #5B4CB1;
}



/*右上功能栏*/
.t_tool {
	z-index: 2;
	font-size: 42rpx;
	position: absolute;
	top: 50%;
	left: 50%;
	-webkit-transform: translate(-50%, -50%);
	        transform: translate(-50%, -50%);
	width: 55rpx;
	height: 53rpx;
	color: #fff;
	text-align: center;
	line-height: 30rpx;
}
#top_tool_1 {
	z-index: 1;
